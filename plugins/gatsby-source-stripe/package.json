{
  "name": "gatsby-source-stripe",
  "version": "2.2.2",
  "main": "index.js",
  "description": "Gatsby source plugin for building websites using Stripe as a data source",
  "scripts": {
    "build": "babel src --out-dir . --copy-files --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "lint": "eslint src --ext .js",
    "format": "prettier --write src/**/**.js",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-source-plugin"
  ],
  "author": "Nathan Beck <njosefbeck@gmail.com> (njosefbeck.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/njosefbeck/gatsby-source-stripe.git"
  },
  "bugs": "https://github.com/njosefbeck/gatsby-source-stripe/issues",
  "license": "MIT",
  "engines": {
    "node": ">=10.13.0"
  },
  "peerDependencies": {
    "gatsby": ">=2.0.15"
  },
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/preset-env": "7.4.3",
    "babel-eslint": "10.0.1",
    "cross-env": "5.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-plugin-import": "2.17.0",
    "eslint-plugin-prettier": "3.0.1",
    "prettier": "1.17.0"
  },
  "dependencies": {
    "gatsby-source-filesystem": "2.0.29",
    "stripe": "6.28.0"
  }
}
